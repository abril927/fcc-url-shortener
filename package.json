{
  "name": "url-shortener",
  "version": "2.0.0",
  "main": "dist/main.js",
  "repository": "https://github.com/abril927/fcc-url-shortener",
  "author": "Abril <abril927@proton.me>",
  "license": "UNLICENSED",
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3"
  },
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "db:push": "dotenv -e .env -c -- prisma db push",
    "db:migrate:dev": "dotenv -e .env -c -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env -c -- prisma migrate prod",
    "db:generate": "dotenv -e .env -c -- prisma generate",
    "db:studio": "dotenv -e .env -c -- prisma studio"
  },
  "devDependencies": {
    "@types/body-parser": "^1",
    "@types/cors": "^2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "dotenv-cli": "^7.4.1",
    "prisma": "^5.10.2",
    "typescript": "^5.4.2"
  },
  "prisma": {
    "schema": "src/schema/schema.prisma"
  }
}
